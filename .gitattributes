# Binary file extensions that should not be modified.
*.axdt binary
*.addin binary
Form.bin binary
*.bin binary
*.png binary
*.ico binary

# Text file extensions
* text=auto
*.htm text eol=crlf
*.html text eol=crlf
renames.txt text eol=crlf
*.bsl  text eol=crlf
*.xml  text eol=crlf
*.feature text
*.md text
*.json text
*.bat text eol=crlf
*.cmd text eol=crlf
*.os text eol=crlf
*.sh text eol=lf
*.groovy text eol=lf
ParentConfigurations.bin text eol=lf

# Git LFS
src/Ext/ParentConfigurations/*.cf filter=lfs diff=lfs merge=lfs -text